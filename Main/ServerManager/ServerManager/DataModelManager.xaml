<UserControl
	x:Uid="UserControl_1" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:UtnEmall.ServerManager="clr-namespace:UtnEmall.ServerManager"
	x:Class="UtnEmall.ServerManager.DataModelManager"
	Width="640" Height="480">

	<Grid x:Uid="Grid_1">
        <Label x:Uid="Label_1" HorizontalAlignment="Left" Margin="8,8,0,0" VerticalAlignment="Top" Width="292" Height="37" Content="{x:Static UtnEmall.ServerManager:UIResources.ManageDataModel}" FontSize="20"/>
		<UtnEmall.ServerManager:ItemList x:Uid="list" Margin="8,45,8,8" x:Name="list" HorizontalAlignment="Stretch" Width="Auto" Height="Auto"/>
        <UtnEmall.ServerManager:BuildProgress x:Uid="buildProgress" x:Name="buildProgress" Width="170" Height="210" HorizontalAlignment="Center" VerticalAlignment="Center" Visibility="Hidden" Margin="0,0,0,0">
            <UtnEmall.ServerManager:BuildProgress.RenderTransform>
                <TransformGroup x:Uid="TransformGroup_1">
                    <ScaleTransform x:Uid="ScaleTransform_1" ScaleX="0.5" ScaleY="0.5"/>
                </TransformGroup>
            </UtnEmall.ServerManager:BuildProgress.RenderTransform>
        </UtnEmall.ServerManager:BuildProgress>
    </Grid>
</UserControl>