<UserControl
	x:Uid="UserControl_1" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2006" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d"
	xmlns:UtnEmall.ServerManager="clr-namespace:UtnEmall.ServerManager"
	x:Class="UtnEmall.ServerManager.CategoryManager"
	Width="640" Height="480">

	<Grid x:Uid="Grid_1">
		<Label x:Uid="Label_1" HorizontalAlignment="Left" Margin="8,8,0,0" VerticalAlignment="Top" Width="292" Height="37" Content="{x:Static UtnEmall.ServerManager:UIResources.ManageCategories}" FontSize="20"/>
		<UtnEmall.ServerManager:Tree x:Uid="tree" Margin="8,45,8,8" x:Name="tree" HorizontalAlignment="Stretch" Width="Auto" Height="Auto"/>
		<Image x:Uid="Image_1" HorizontalAlignment="Right" Margin="200,0,0,100" Width="Auto" Source="imgs\categories_mono.png" Stretch="Fill"/>
	</Grid>
</UserControl>