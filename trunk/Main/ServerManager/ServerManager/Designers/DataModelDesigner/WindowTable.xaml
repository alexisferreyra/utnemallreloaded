		<GroupBox x:Uid="GroupBox_2" Margin="18.6,4,0,0" VerticalAlignment="Top" Height="49.6" Header="{x:Static UtnEmall.ServerManager:UIResources.Table}" HorizontalAlignment="Left" Width="314.4" />
		<TextBox TabIndex="0" x:Uid="textTableName" Margin="110.203,23,0,0" VerticalAlignment="Top" Height="23" Text="" TextWrapping="Wrap" x:Name="textTableName" HorizontalAlignment="Left" Width="212.797" />
		<CheckBox TabIndex="3" x:Uid="checkIsStorage" x:Name="checkIsStorage" Margin="94,155,0,0" Content="Storage" VerticalAlignment="Top" Height="16" HorizontalAlignment="Left" Width="85" />
