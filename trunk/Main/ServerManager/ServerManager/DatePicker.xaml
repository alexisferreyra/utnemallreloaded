<UserControl
	x:Uid="UserControl_1" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:Class="UtnEmall.ServerManager.DatePicker"
	Width="200" Height="28">

	<UniformGrid x:Uid="UniformGrid_1" Rows="1" Columns="3" Height="24">
		<ComboBox x:Uid="First" IsSynchronizedWithCurrentItem="True" Width="Auto" Height="Auto" x:Name="First" Padding="4,3,8,3" Margin="0,0,2,0"/>
		<ComboBox x:Uid="Second" IsSynchronizedWithCurrentItem="True" Width="Auto" Height="Auto" x:Name="Second" Padding="4,3,8,3" Margin="2,0,2,0"/>
		<ComboBox x:Uid="Third" IsSynchronizedWithCurrentItem="True" Width="Auto" Height="Auto" x:Name="Third" Margin="2,0,0,0"/>
	</UniformGrid>
</UserControl>