		<Button TabIndex="9" x:Uid="ButtonCancel" d:LayoutOverrides="Height" Margin="261.977,232,0,0" x:Name="ButtonCancel" VerticalAlignment="Top" Content="{x:Static UtnEmall.ServerManager:UIResources.Cancel}" Click="ButtonCancel_Click" HorizontalAlignment="Left" Width="72.023" Height="21.96" />
		<Button TabIndex="8" x:Uid="buttonOK" d:LayoutOverrides="Height" Margin="189.977,232,0,0" x:Name="buttonOK" VerticalAlignment="Top" Content="{x:Static UtnEmall.ServerManager:UIResources.OK}" Click="buttonOK_Click" Height="21.96" HorizontalAlignment="Left" Width="66.023" />
		<Button TabIndex="5" x:Uid="buttonRemove" x:Name="buttonRemove" Margin="110.203,188.04,0,0" VerticalAlignment="Top" Click="buttonRemove_Click" Content="{x:Static UtnEmall.ServerManager:UIResources.Remove}" Height="21.96" HorizontalAlignment="Left" Width="84.8" />
