<UserControl
	x:Uid="UserControl_1" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:Janus="clr-namespace:Janus"
	x:Class="ServerManager.Statistics.StatisticsViewer"
	Width="640" Height="480">

	<Grid x:Uid="Grid_1">
		<Image HorizontalAlignment="Right" Margin="200,0,0,100" Width="Auto" Source="..\imgs\statistics_mono.png" Stretch="Fill"/>
		<Label x:Uid="Label_1" HorizontalAlignment="Left" Margin="8,8,0,0" VerticalAlignment="Top" Width="292" Height="37" Content="View statistics" FontSize="20"/>
        <Label x:Uid="label1" HorizontalAlignment="Left" Margin="75,0,0,200" x:Name="label1" Width="47" Content="Service" Height="23" VerticalAlignment="Bottom" />
        <ComboBox x:Uid="comboBoxService" HorizontalAlignment="Left" Margin="125,0,0,200" x:Name="comboBoxService" Width="133" Height="23" VerticalAlignment="Bottom" />
		<Button x:Uid="buttonViewServiceStatistics" Margin="75,0,0,155" x:Name="buttonViewServiceStatistics" Content="View Service Statistics" HorizontalAlignment="Left" Width="183" Click="OnViewServiceStatisticsClicked" Height="30" VerticalAlignment="Bottom" />
		<Button x:Uid="buttonViewGlobalStatisticsAnalysis" Height="30" Margin="75,93,0,0" x:Name="buttonViewGlobalStatisticsAnalysis" VerticalAlignment="Top" HorizontalAlignment="Left" Width="183" Content="View Global Statistics Analysis" Click="OnGlobalStatisticsAnalyzerClicked"/>
	</Grid>
</UserControl>